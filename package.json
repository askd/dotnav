{
  "name": "dotnav",
  "version": "0.0.0",
  "description": "Dots Navigation for React Components",
  "main": "index.js",
  "files": [
    "*.md",
    "lib/*.js",
    "lib/*.css"
  ],
  "scripts": {
    "clean": "rimraf lib/*.js && rimraf lib/*.css",
    "build:js": "babel -s inline -d ./ **/*.es6",
    "build:css": "postcss lib/Dotnav.pcss -u autoprefixer -o ./lib/Dotnav.css",
    "build": "npm run clean && npm run build:js && npm run build:css",
    "prepublish": "npm run build",
    "test": "$(npm bin)/jest"
  },
  "author": "Anna Selezniova",
  "license": "MIT",
  "keywords": [
    "dots",
    "arrows",
    "navigation",
    "react",
    "react-component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/askd/dotnav"
  },
  "homepage": "https://animakit.github.io",
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "jest": "^19.0.2",
    "jsdom": "^9.11.0",
    "postcss-cli": "^3.2.0",
    "prop-types": "^15.5.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "react": "^15.5.0",
    "react-dom": "^15.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
